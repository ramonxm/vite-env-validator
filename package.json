  "peerDependencies": {
    "zod": "^3.0.0 || ^4.0.0",
    "yup": "^1.0.0",
    "joi": "^17.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
